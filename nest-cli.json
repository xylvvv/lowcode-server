{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/lowcode-server/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/lowcode-server/tsconfig.app.json",
    "assets": [
      "**/*.proto"
    ],
    "watchAssets": true
  },
  "monorepo": true,
  "root": "apps/lowcode-server",
  "generateOptions": {
    "spec": false
  },
  "projects": {
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json"
      }
    },
    "lowcode-server": {
      "type": "application",
      "root": "apps/lowcode-server",
      "entryFile": "main",
      "sourceRoot": "apps/lowcode-server/src",
      "compilerOptions": {
        "tsConfigPath": "apps/lowcode-server/tsconfig.app.json"
      }
    },
    "work": {
      "type": "application",
      "root": "apps/work",
      "entryFile": "main",
      "sourceRoot": "apps/work/src",
      "compilerOptions": {
        "tsConfigPath": "apps/work/tsconfig.app.json"
      }
    },
    "channel": {
      "type": "application",
      "root": "apps/channel",
      "entryFile": "main",
      "sourceRoot": "apps/channel/src",
      "compilerOptions": {
        "tsConfigPath": "apps/channel/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "third-party": {
      "type": "application",
      "root": "apps/third-party",
      "entryFile": "main",
      "sourceRoot": "apps/third-party/src",
      "compilerOptions": {
        "tsConfigPath": "apps/third-party/tsconfig.app.json"
      }
    },
    "library": {
      "type": "application",
      "root": "apps/library",
      "entryFile": "main",
      "sourceRoot": "apps/library/src",
      "compilerOptions": {
        "tsConfigPath": "apps/library/tsconfig.app.json"
      }
    }
  }
}